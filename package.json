{
  "name": "domiurge",
  "description": "CLI for management docker based microservices apps",
  "version": "0.3.7",
  "author": "Mateusz Cholewka",
  "bin": {
    "domiurge": "./bin/run"
  },
  "bugs": "https://github.com/mtk3d/domiurge/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/execa": "^0.9.0",
    "@types/js-yaml": "^3.12.1",
    "@types/listr": "^0.13.0",
    "ajv": "^6.10.0",
    "cli-color": "^1.4.0",
    "execa": "^1.0.0",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "slugify": "^1.3.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mtk3d/domiurge",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "domiurge",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/check-config",
        "./lib/hooks/init/check-dependencies"
      ]
    }
  },
  "repository": "mtk3d/domiurge",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "test": "echo 'no test'"
  },
  "types": "lib/index.d.ts"
}
